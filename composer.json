{
  "repositories":[
    {
      "type" : "composer",
      "url" : "http://wpackagist.org"
    },
    {
      "type" : "vcs",
      "url" : "https://github.com/francoistibo/acf-repeater-plugin"
    }
  ],
  "require": {
    "johnpbloch/wordpress": "^4.5",
    "wpackagist-plugin/duplicate-post": "^2.6",
    "wpackagist-plugin/imsanity": "^2.3.5",
    "wpackagist-plugin/intuitive-custom-post-order": "^3.0.7",
    "wpackagist-plugin/loco-translate": "^1.5",
    "wpackagist-plugin/login-lockdown": "1.6",
    "wpackagist-plugin/regenerate-thumbnails": "^2.2.4",
    "wpackagist-plugin/tinymce-advanced": "^4.2",
    "wpackagist-plugin/wordpress-importer": "^0.6.1",
    "wpackagist-plugin/wordpress-seo": "^3.1",
    "wpackagist-plugin/wp-super-cache": "^1.4.6",
    "acf-repeater-plugin": "^1.0"
  },
  "extra": {
    "webroot-dir": "wordpress",
    "webroot-package": "wordpress"
  },
  "scripts": {
      "post-install-cmd": [
          "gulp compile"
      ],
      "post-update-cmd": [
          "gulp compile"
      ]
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
